<launch>
    <!-- Arguments -->
    <arg name="chassis_model" default="$(optenv LINGAO_MODEL MiniUGV-10A)" doc="model type [MiniUGV-10A, MiniUGV-20A, LAUGV-50A ...]"/>
    <arg name="lidar_type" default="$(optenv LINGAO_LIDAR rplidar)" doc="model type [rplidar, ltme02a, rslidar ...]"/>

    <!-- Run lingao compatible laser drivers. -->
    <include file="$(find lingao_bringup)/launch/include/lidar/$(arg lidar_type).launch" >
        <arg name="chassis_model" value="$(arg chassis_model)"/>
    </include>

</launch>


